package kosta.service.dice;

public class DiceA {
	int value;

	public DiceA(int value) {
		super();
		this.value = value;
	}

	public int getValue() {
		return value;
	}

	public void setValue(int value) {
		this.value = value;
	}

	public int seletedNumber() {
	for(int i=0; i<6; i++) {
		randomIndex = (int)(Math.random()*7);
		
		} return randomIndex;
	}
	@Override
	public String toString() {
		return "DiceA [value=" + value + "]";
	}

	
}

int randomIndex = 0;
int temp[] = new int[30];
for (int i = 0; i < students.length; i++){
	temp = new int[30];
	for(int j = 0; j < 30; j++){
		randomIndex = (int)((Math.random()*10 + 1))%3;
		temp[j] = randomIndex;
	}
	students[i].setAttendances(temp);

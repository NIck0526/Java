package file.step1.test;

import java.io.File;

public class FileMethodTestSelf3 {
	public static void main(String[] args) {
		File dir1 = new File("c:\\filetest\\step1\\code");
		File dir2 = new File("c:\\filetest2");
		
		System.out.println("叼泛配府 积己?"+dir1.isDirectory());
		
		dir1.mkdirs();
		dir2.mkdirs();

		System.out.println("叼泛配府 积己?"+dir1.isDirectory());
		System.out.println("叼泛配府 积己?"+dir2.isDirectory());
		File fp = dir1.getParentFile();
		
		File[] files = fp.listFiles();
		for(File f : files) {
			if(f.isFile())
				System.out.println(f.getName());
			else(f.isDirectory())
			System.out.println("いいいいい");
		
		}
		
	}		
}	